mixin checkbox(input_id, input_label)
    div.checkbox_lab
        input(class= 'check' id= input_id type= 'checkbox' value= input_id)
        label(class= 'lab' for= input_id)!= input_label
mixin checkbox_simple(input_id, input_name)
    input(class= 'check' id=input_id type='checkbox' value=input_name)